HOST="https://homes.cs.washington.edu"
ROUTE="~tschweiz/research/en2bash-study/backend/post_handler/post_handler.php"

USER_ID="test"
TASK_ORDER=T1N2
CLIENT_TIME_STAMP=$(date --utc +%FT%TZ)
TASK_NO=12
TREATMENT="NoTellina"
COMMAND="find . -type f -name \"*.txt\""
TIME_ELAPSED=$SECONDS
STATUS="success"

curl -X POST ${HOST}/${ROUTE} \
  -d c="$CLIENT_TIME_STAMP" \
  -d a="$USER_ID" \
  -d b="$TASK_ORDER" \
  -d d="$TASK_NO" \
  -d e="$TREATMENT" \
  -d g="$TIME_ELAPSED" \
  -d h="$STATUS" \
  -d f="$COMMAND"
